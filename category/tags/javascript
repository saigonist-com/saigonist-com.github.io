    calculate_tomorrow<span style="color: #009900;">&#40;</span>today<span style="color: #339933;">,</span> tomorrow<span style="color: #009900;">&#41;</span><span style="color: #339933;">;</span>
    print_r<span style="color: #009900;">&#40;</span>tomorrow<span style="color: #009900;">&#41;</span><span style="color: #339933;">;</span>
